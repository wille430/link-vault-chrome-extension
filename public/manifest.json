{
    "name": "Link Vault",
    "description": "Save and organize links",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "Popup.html"
    },
    "host_permissions": [
        "http://localhost:8080/*"
    ],
    "background": {
        "service_worker": "./static/js/background.js"
    }
}