{
    "name": "Link Vault",
    "description": "Save and organize links",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "scripting",
        "storage",
        "*://*.dropboxapi.com/*"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "static/js/background.js"
        ]
    },
    "icons": {
        "256": "images/link-vault-256.png",
        "128": "images/link-vault-128.png",
        "48": "images/link-vault-48.png",
        "16": "images/link-vault-16.png"
    }
}